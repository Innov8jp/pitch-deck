    <script>
        // Presentation Engine
        class PresentationEngine {
            constructor() {
                this.currentSlide = 1;
                this.totalSlides = 0;
                this.slides = [];
                this.isTransitioning = false;
                this.touchStartX = 0;
                this.touchEndX = 0;
                
                this.init();
            }

            init() {
                // Get all slides
                this.slides = document.querySelectorAll('.slide');
                this.totalSlides = this.slides.length;
                
                // Update UI
                this.updateUI();
                
                // Event Listeners
                this.setupEventListeners();
                
                // Initialize slide positions
                this.initializeSlides();
            }

            initializeSlides() {
                this.slides.forEach((slide, index) => {
                    if (index < this.currentSlide - 1) {
                        slide.classList.add('prev');
                    } else if (index === this.currentSlide - 1) {
                        slide.classList.add('active');
                    }
                });
            }

            setupEventListeners() {
                // Keyboard Navigation
                document.addEventListener('keydown', (e) => this.handleKeyPress(e));
                
                // Button Navigation
                document.getElementById('prevBtn').addEventListener('click', () => this.previousSlide());
                document.getElementById('nextBtn').addEventListener('click', () => this.nextSlide());
                
                // Touch Navigation
                const container = document.getElementById('presentation');
                container.addEventListener('touchstart', (e) => this.handleTouchStart(e));
                container.addEventListener('touchend', (e) => this.handleTouchEnd(e));
                
                // Fullscreen
                document.getElementById('fullscreenBtn').addEventListener('click', () => this.toggleFullscreen());
                
                // Mouse Wheel Navigation (optional)
                container.addEventListener('wheel', (e) => this.handleWheel(e), { passive: false });
            }

            handleKeyPress(e) {
                switch(e.key) {
                    case 'ArrowLeft':
                        e.preventDefault();
                        this.previousSlide();
                        break;
                    case 'ArrowRight':
                    case ' ':
                        e.preventDefault();
                        this.nextSlide();
                        break;
                    case 'Home':
                        e.preventDefault();
                        this.goToSlide(1);
                        break;
                    case 'End':
                        e.preventDefault();
                        this.goToSlide(this.totalSlides);
                        break;
                    case 'f':
                    case 'F':
                        e.preventDefault();
                        this.toggleFullscreen();
                        break;
                    case 'Escape':
                        if (document.fullscreenElement) {
                            document.exitFullscreen();
                        }
                        break;
                }
            }

            handleTouchStart(e) {
                this.touchStartX = e.changedTouches[0].screenX;
            }

            handleTouchEnd(e) {
                this.touchEndX = e.changedTouches[0].screenX;
                this.handleSwipe();
            }

            handleSwipe() {
                const swipeThreshold = 50;
                const diff = this.touchStartX - this.touchEndX;
                
                if (Math.abs(diff) > swipeThreshold) {
                    if (diff > 0) {
                        this.nextSlide();
                    } else {
                        this.previousSlide();
                    }
                }
            }

            handleWheel(e) {
                if (Math.abs(e.deltaY) > Math.abs(e.deltaX)) {
                    e.preventDefault();
                    if (e.deltaY > 0) {
                        this.nextSlide();
                    } else {
                        this.previousSlide();
                    }
                }
            }

            nextSlide() {
                if (this.currentSlide < this.totalSlides && !this.isTransitioning) {
                    this.goToSlide(this.currentSlide + 1);
                }
            }

            previousSlide() {
                if (this.currentSlide > 1 && !this.isTransitioning) {
                    this.goToSlide(this.currentSlide - 1);
                }
            }

            goToSlide(slideNumber) {
                if (slideNumber < 1 || slideNumber > this.totalSlides || this.isTransitioning) {
                    return;
                }

                this.isTransitioning = true;

                // Remove active class from current slide
                this.slides[this.currentSlide - 1].classList.remove('active');
                
                // Update slide classes
                this.slides.forEach((slide, index) => {
                    slide.classList.remove('active', 'prev');
                    
                    if (index < slideNumber - 1) {
                        slide.classList.add('prev');
                    } else if (index === slideNumber - 1) {
                        slide.classList.add('active');
                    }
                });

                this.currentSlide = slideNumber;
                this.updateUI();

                // Reset transition flag
                setTimeout(() => {
                    this.isTransitioning = false;
                }, 600);
            }

            updateUI() {
                // Update slide counter
                document.getElementById('currentSlide').textContent = this.currentSlide;
                document.getElementById('totalSlides').textContent = this.totalSlides;
                
                // Update progress bar
                const progress = (this.currentSlide / this.totalSlides) * 100;
                document.getElementById('progressFill').style.width = progress + '%';
                
                // Update navigation buttons
                document.getElementById('prevBtn').disabled = this.currentSlide === 1;
                document.getElementById('nextBtn').disabled = this.currentSlide === this.totalSlides;
            }

            toggleFullscreen() {
                const elem = document.getElementById('presentation');
                
                if (!document.fullscreenElement) {
                    elem.requestFullscreen().catch(err => {
                        console.log(`Error attempting to enable fullscreen: ${err.message}`);
                    });
                } else {
                    document.exitFullscreen();
                }
            }
        }

        // Interactive Agent Cards with Constellation Map
        class AgentInteractions {
            constructor() {
                this.activeAgent = null;
                this.currentStyle = 'constellation'; // holographic, constellation, lightning
                this.constellationPositions = [
                    { x: 0, y: -150 },       // Top
                    { x: 150, y: 0 },        // Right  
                    { x: 80, y: 120 },       // Bottom Right
                    { x: -120, y: 80 }       // Bottom Left
                ];
                this.init();
            }

            init() {
                // Setup agent card interactions
                const agentCards = document.querySelectorAll('.interactive-agent-card');
                agentCards.forEach(card => {
                    // Hover effects
                    card.addEventListener('mouseenter', (e) => this.handleCardHover(e));
                    card.addEventListener('mouseleave', (e) => this.handleCardLeave(e));
                    
                    // Click interactions
                    card.addEventListener('click', (e) => this.handleCardClick(e));
                });

                // Close floating orbs when clicking outside
                document.addEventListener('click', (e) => {
                    if (!e.target.closest('.interactive-agent-card')) {
                        this.hideAllPowerOrbs();
                    }
                });
            }

            handleCardHover(e) {
                const card = e.currentTarget;
                const shimmer = card.querySelector('.shimmer-effect');
                
                // Trigger shimmer effect
                shimmer.style.left = '100%';
                
                // Enhanced hover animation
                card.style.transform = 'translateY(-10px) scale(1.02)';
                card.style.borderColor = 'rgba(255, 255, 255, 0.3)';
                card.style.boxShadow = '0 20px 60px rgba(0, 0, 0, 0.3)';
            }

            handleCardLeave(e) {
                const card = e.currentTarget;
                const shimmer = card.querySelector('.shimmer-effect');
                
                // Reset shimmer
                setTimeout(() => {
                    shimmer.style.left = '-100%';
                }, 100);
                
                // Reset hover animation if not active
                if (this.activeAgent !== card.dataset.agent) {
                    card.style.transform = 'translateY(0) scale(1)';
                    card.style.borderColor = 'rgba(255, 255, 255, 0.1)';
                    card.style.boxShadow = 'none';
                }
            }

            handleCardClick(e) {
                e.stopPropagation();
                const card = e.currentTarget;
                const agentType = card.dataset.agent;
                
                if (this.activeAgent === agentType) {
                    // If clicking the same card, close it
                    this.hidePowerOrbs(agentType);
                    this.activeAgent = null;
                } else {
                    // Hide any currently active orbs
                    this.hideAllPowerOrbs();
                    
                    // Show new constellation
                    this.showConstellation(agentType);
                    this.activeAgent = agentType;
                }
            }

            createStarfield(canvas, color) {
                if (!canvas) return;
                
                const ctx = canvas.getContext('2d');
                const width = canvas.width;
                const height = canvas.height;
                
                // Clear canvas
                ctx.clearRect(0, 0, width, height);
                
                // Create stars
                for (let i = 0; i < 100; i++) {
                    const x = Math.random() * width;
                    const y = Math.random() * height;
                    const size = Math.random() * 2;
                    const opacity = Math.random() * 0.8 + 0.2;
                    
                    ctx.fillStyle = `rgba(255, 255, 255, ${opacity})`;
                    ctx.beginPath();
                    ctx.arc(x, y, size, 0, Math.PI * 2);
                    ctx.fill();
                }
                
                // Add some colored stars based on agent
                for (let i = 0; i < 20; i++) {
                    const x = Math.random() * width;
                    const y = Math.random() * height;
                    const size = Math.random() * 1.5 + 0.5;
                    const opacity = Math.random() * 0.6 + 0.4;
                    
                    ctx.fillStyle = color.replace('0.8', opacity.toString());
                    ctx.beginPath();
                    ctx.arc(x, y, size, 0, Math.PI * 2);
                    ctx.fill();
                }
            }

            drawConstellationLines(svg, positions, color) {
                if (!svg) return;
                
                // Clear existing lines
                svg.innerHTML = svg.innerHTML.split('<!-- Lines will be drawn dynamically -->')[0] + '<!-- Lines will be drawn dynamically -->';
                
                const center = { x: 300, y: 300 }; // Center of 600x600 container
                
                // Draw lines from center to each position
                positions.forEach((pos, index) => {
                    const targetX = center.x + pos.x;
                    const targetY = center.y + pos.y;
                    
                    const line = document.createElementNS("http://www.w3.org/2000/svg", "line");
                    line.setAttribute("x1", center.x);
                    line.setAttribute("y1", center.y);
                    line.setAttribute("x2", targetX);
                    line.setAttribute("y2", targetY);
                    line.setAttribute("stroke", `url(#lineGradient-${this.activeAgent})`);
                    line.setAttribute("stroke-width", "2");
                    line.setAttribute("filter", `url(#constellation-glow-${this.activeAgent})`);
                    line.setAttribute("opacity", "0");
                    
                    svg.appendChild(line);
                    
                    // Animate line appearance
                    setTimeout(() => {
                        line.setAttribute("opacity", "0.8");
                        line.style.transition = "opacity 0.8s ease";
                    }, index * 200);
                });

                // Draw connecting lines between nodes
                for (let i = 0; i < positions.length; i++) {
                    const nextIndex = (i + 1) % positions.length;
                    const pos1 = positions[i];
                    const pos2 = positions[nextIndex];
                    
                    const line = document.createElementNS("http://www.w3.org/2000/svg", "line");
                    line.setAttribute("x1", center.x + pos1.x);
                    line.setAttribute("y1", center.y + pos1.y);
                    line.setAttribute("x2", center.x + pos2.x);
                    line.setAttribute("y2", center.y + pos2.y);
                    line.setAttribute("stroke", color);
                    line.setAttribute("stroke-width", "1");
                    line.setAttribute("opacity", "0");
                    line.setAttribute("stroke-dasharray", "5,5");
                    
                    svg.appendChild(line);
                    
                    // Animate connecting lines
                    setTimeout(() => {
                        line.setAttribute("opacity", "0.4");
                        line.style.transition = "opacity 1s ease";
                    }, 1000 + (i * 300));
                }
            }

            showConstellation(agentType) {
                const orbContainer = document.querySelector(`.power-orbs-container[data-agent="${agentType}"]`);
                const card = document.querySelector(`.interactive-agent-card[data-agent="${agentType}"]`);
                
                if (!orbContainer || !card) return;
                
                const starfield = orbContainer.querySelector('.starfield');
                const canvas = orbContainer.querySelector('.star-canvas');
                const constellationLines = orbContainer.querySelector('.constellation-lines');
                const starNodes = orbContainer.querySelectorAll('.star-node');
                const centralStar = orbContainer.querySelector('.central-star');
                
                // Enhance the main card appearance
                card.style.transform = 'translateY(-15px) scale(1.08)';
                card.style.borderColor = this.getAgentColor(agentType);
                card.style.boxShadow = `0 30px 100px ${this.getAgentColor(agentType)}60`;
                card.style.zIndex = '100';
                card.style.animation = 'cosmicGlow 3s ease-in-out infinite';
                
                // Show starfield
                if (starfield) {
                    starfield.style.opacity = '1';
                    this.createStarfield(canvas, this.getAgentColor(agentType));
                }
                
                // Show central star
                if (centralStar) {
                    centralStar.style.opacity = '1';
                }
                
                // Show constellation lines
                if (constellationLines) {
                    constellationLines.style.opacity = '1';
                    setTimeout(() => {
                        this.drawConstellationLines(constellationLines, this.constellationPositions, this.getAgentColor(agentType));
                    }, 500);
                }
                
                // Animate star nodes
                starNodes.forEach((node, index) => {
                    const delay = index * 400 + 800; // Delay after lines start appearing
                    
                    setTimeout(() => {
                        node.style.opacity = '1';
                        node.style.animation = 'starBirth 1.5s ease-out forwards';
                        
                        // Start twinkling effect
                        setTimeout(() => {
                            const twinkle = node.querySelector('.star-twinkle');
                            if (twinkle) {
                                twinkle.style.animation = 'starTwinkle 3s ease-in-out infinite';
                            }
                            
                            // Add cosmic glow animation
                            node.style.animation = 'starBirth 1.5s ease-out forwards, cosmicGlow 4s ease-in-out infinite 1.5s';
                        }, 1500);
                    }, delay);
                });
            }

            hidePowerOrbs(agentType) {
                const orbContainer = document.querySelector(`.power-orbs-container[data-agent="${agentType}"]`);
                const card = document.querySelector(`.interactive-agent-card[data-agent="${agentType}"]`);
                
                if (!orbContainer || !card) return;
                
                const starfield = orbContainer.querySelector('.starfield');
                const constellationLines = orbContainer.querySelector('.constellation-lines');
                const starNodes = orbContainer.querySelectorAll('.star-node');
                const centralStar = orbContainer.querySelector('.central-star');
                
                // Reset main card appearance
                card.style.transform = 'translateY(0) scale(1)';
                card.style.borderColor = 'rgba(255, 255, 255, 0.1)';
                card.style.boxShadow = 'none';
                card.style.zIndex = 'auto';
                card.style.animation = 'none';
                
                // Hide elements
                if (starfield) {
                    starfield.style.opacity = '0';
                }
                
                if (centralStar) {
                    centralStar.style.opacity = '0';
                }
                
                if (constellationLines) {
                    constellationLines.style.opacity = '0';
                }
                
                // Hide star nodes
                starNodes.forEach((node, index) => {
                    const delay = index * 100;
                    
                    setTimeout(() => {
                        node.style.animation = 'none';
                        node.style.transform = 'translate(-50%, -50%) scale(0)';
                        node.style.opacity = '0';
                        
                        const twinkle = node.querySelector('.star-twinkle');
                        if (twinkle) {
                            twinkle.style.animation = 'none';
                        }
                    }, delay);
                });
            }

            hideAllPowerOrbs() {
                const allOrbContainers = document.querySelectorAll('.power-orbs-container');
                const allCards = document.querySelectorAll('.interactive-agent-card');
                
                allOrbContainers.forEach(container => {
                    const starfield = container.querySelector('.starfield');
                    const constellationLines = container.querySelector('.constellation-lines');
                    const starNodes = container.querySelectorAll('.star-node');
                    const centralStar = container.querySelector('.central-star');
                    
                    // Hide all elements
                    if (starfield) starfield.style.opacity = '0';
                    if (centralStar) centralStar.style.opacity = '0';
                    if (constellationLines) constellationLines.style.opacity = '0';
                    
                    starNodes.forEach((node, index) => {
                        const delay = index * 80;
                        setTimeout(() => {
                            node.style.animation = 'none';
                            node.style.transform = 'translate(-50%, -50%) scale(0)';
                            node.style.opacity = '0';
                            
                            const twinkle = node.querySelector('.star-twinkle');
                            if (twinkle) {
                                twinkle.style.animation = 'none';
                            }
                        }, delay);
                    });
                });
                
                allCards.forEach(card => {
                    card.style.transform = 'translateY(0) scale(1)';
                    card.style.borderColor = 'rgba(255, 255, 255, 0.1)';
                    card.style.boxShadow = 'none';
                    card.style.zIndex = 'auto';
                    card.style.animation = 'none';
                });
                
                this.activeAgent = null;
            }

            getAgentColor(agentType) {
                const colors = {
                    'sparky': 'rgba(251, 191, 36, 0.8)',
                    'inspector': 'rgba(6, 182, 212, 0.8)',
                    'penny': 'rgba(139, 92, 246, 0.8)',
                    'captain': 'rgba(5, 150, 105, 0.8)'
                };
                return colors[agentType] || 'rgba(255, 255, 255, 0.3)';
            }
        }

        // Initialize presentation when DOM is loaded
        document.addEventListener('DOMContentLoaded', () => {
            new PresentationEngine();
            new AgentInteractions();
        });

        // Prevent default touch behavior on iOS
        document.addEventListener('touchmove', (e) => {
            if (e.touches.length === 1) {
                e.preventDefault();
            }
        }, { passive: false });
    </script>
</body>
</html>
